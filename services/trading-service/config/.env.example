# TimescaleDB (读取K线)
DATABASE_URL=postgresql://user:password@localhost:5432/market_data

# SQLite (写入指标结果)
INDICATOR_SQLITE_PATH=/path/to/tradecat/libs/database/services/telegram-service/market_data.db

# 计算参数
MAX_WORKERS=4
COMPUTE_BACKEND=thread

# ========== 币种管理 ==========
# 使用的分组（逗号分隔可多选，特殊值: auto/all）
SYMBOLS_GROUPS=auto

# 自定义分组
SYMBOLS_GROUP_main=BTCUSDT,ETHUSDT,BNBUSDT,SOLUSDT,XRPUSDT,ADAUSDT,DOGEUSDT,AVAXUSDT,DOTUSDT,LINKUSDT
SYMBOLS_GROUP_defi=AAVEUSDT,UNIUSDT,MKRUSDT,COMPUSDT,SNXUSDT,CRVUSDT,LDOUSDT,PENDLEUSDT
SYMBOLS_GROUP_meme=DOGEUSDT,SHIBUSDT,PEPEUSDT,FLOKIUSDT,BONKUSDT,WIFUSDT,BOMEUSDT
SYMBOLS_GROUP_ai=FETUSDT,AGIXUSDT,OCEANUSDT,RENDERUSDT,TAOUSDT,ARKMUSDT
SYMBOLS_GROUP_layer2=ARBUSDT,OPUSDT,MATICUSDT,STXUSDT,IMXUSDT,METISUSDT

# 额外添加
SYMBOLS_EXTRA=

# 强制排除
SYMBOLS_EXCLUDE=

# 高优先级数量（auto模式生效）
HIGH_PRIORITY_TOP_N=50
